<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.meirinaldojunior.eporaqui_vactivit"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="23" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.meirinaldojunior.eporaqui_vactivit.Inicial" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.meirinaldojunior.eporaqui_vactivit.LoginTurista"
            android:label="@string/title_activity_login_turista"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.meirinaldojunior.eporaqui_vactivit.LoginGuia"
            android:label="@string/title_activity_login_guia"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.meirinaldojunior.eporaqui_vactivit.cadastros.Cad_guia"
            android:label="@string/title_activity_cad_guia"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.meirinaldojunior.eporaqui_vactivit.cadastros.Cad_turista"
            android:label="@string/title_activity_cad_turista"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>